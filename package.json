{
  "name": "@techassi/vue-lazy-image",
  "version": "1.0.1",
  "description": "Lazy load images in Vue 2/3 backed by the IntersectionObserver API",
  "author": "Techassi <git@techassi.dev>",
  "keywords": [
    "vue",
    "image",
    "picture",
    "intersection-observer"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint:fix": "pnpm lint --fix",
    "play": "cd playgrounds && pnpm dev",
    "build": "vue-tsc --noEmit && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Techassi/vue-lazy-image.git",
    "directory": "src"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/Techassi/vue-lazy-image/issues"
  },
  "homepage": "https://github.com/Techassi/vue-lazy-image#readme",
  "peerDependencies": {
    "@vue/composition-api": "^1.1.0",
    "vue": "^2.7.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@techassi/eslint-config": "^0.0.9",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.35.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2",
    "vue": "^3.0.0",
    "vue-tsc": "^1.2.0",
    "vue2": "npm:vue@^2.7.13"
  },
  "dependencies": {
    "vue-demi": "latest"
  }
}